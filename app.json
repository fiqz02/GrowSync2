{
  "expo": {
    "name": "GrowSync",
    "slug": "GrowSync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoGS(2).png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoGS(1).png",
      "resizeMode": "contain",
      "backgroundColor": "#87cefa"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apitx.GrowSync"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.apitz.GrowSync"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58ff3a23-6139-4bf7-8bc2-888e3c85cd97"
      }
    }
  }
}
